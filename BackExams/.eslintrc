{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": [
      "error", { "argsIgnorePattern": "next" }
    ],
    "class-methods-use-this": [0],
    "no-param-reassign": [0],
    "no-plusplus": [0],
    "import/newline-after-import": [0]
  }
}